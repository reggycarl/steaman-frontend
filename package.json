{
  "name": "steaman-commerce",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "jest-transform-css"
    }
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next build && next start",
    "test": "jest"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "axios": "^0.21.0",
    "babel-jest": "^27.4.5",
    "bootstrap": "^4.5.3",
    "install": "^0.13.0",
    "jest": "^27.4.5",
    "jest-dom": "^4.0.0",
    "jest-transform-css": "^3.0.0",
    "lodash": "^4.17.20",
    "next": "10.0.3",
    "next-testing": "^0.0.0",
    "nprogress": "^0.2.0",
    "query-string": "^6.13.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-ga": "^3.3.0",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.1.0",
    "react-image-gallery": "^1.0.9",
