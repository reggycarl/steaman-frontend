    "react-redux": "^7.2.2",
    "react-select": "^3.1.1",
    "react-simple-image-slider": "^2.0.1",
    "react-slick": "^0.27.13",
    "react-spinners": "^0.10.4",
    "react-toastify": "^6.1.0",
    "react-zoom-slider": "^0.1.0",
    "reactjs-freshchat": "^1.3.0",
